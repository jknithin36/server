  "license": "ISC",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc -p tsconfig.json",
    "start": "node --enable-source-maps dist/index.js",
    "db:migrate": "psql \"$DATABASE_URL\" -f db/migrations/001_create_rules.sql",
    "db:seed": "tsx scripts/seed.ts data/rules.json",
    "db:truncate": "psql \"$DATABASE_URL\" -c \"TRUNCATE rules\"",
    "db:reseed": "npm run db:truncate && npm run db:seed",
    "smoke": "bash scripts/smoke.sh"
  },
  "dependencies": {
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "express": "^5.1.0",
    "express-rate-limit": "^8.0.1",
    "helmet": "^8.1.0",
    "morgan": "^1.10.1",
    "pg": "^8.16.3",
    "zod": "^4.1.5"
  },
  "devDependencies": {
    "@types/express": "^5.0.3",
    "@types/node": "^24.3.0",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2"
  }
}
